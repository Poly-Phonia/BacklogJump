{
    "manifest_version": 3,
    "name": "Backlog Jump",
    "version": "0.1.0",
    "action": {
        "default_title": "setting",
        "default_popup": "popup.html"
    },
    "permissions":[
        "storage",
        "contextMenus",
        "tabs"
    ],
    "icons":{
        "16": "assets/icon/icon16.png",
        "32": "assets/icon/icon32.png",
        "48": "assets/icon/icon48.png",
        "128": "assets/icon/icon128.png"
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js":["selection-notice.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}